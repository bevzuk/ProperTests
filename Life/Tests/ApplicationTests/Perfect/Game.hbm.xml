<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ApplicationTests.Perfect" assembly="ApplicationTests" default-access="field" default-lazy="false" default-cascade="all" auto-import="false">

  <class name="Game" table="Game">
    <id name="Name" column="Name" access="property" />
    <property name="Size" column="Size" access="property" />
    
    <bag name="cells" lazy="true" inverse="true">
      <key column="GameId" />
      <one-to-many class="Cell" />
    </bag>  
  </class>

</hibernate-mapping>