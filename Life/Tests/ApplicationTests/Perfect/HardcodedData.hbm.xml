<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ApplicationTests.Perfect" assembly="ApplicationTests" default-access="field" default-lazy="false" default-cascade="all" auto-import="false">

  <class name="HardcodedData" table="sys">
    <id type="int" column="sys_id">
      <generator class="native" />
    </id>

    <bag name="games" table="Game" lazy="true">
      <key column="sys_id" />
      <many-to-many class="Game" column="GameId" />
    </bag>   
  </class>

</hibernate-mapping>